{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(|index.html)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    },
    {
      "source": "/(.*)\\.(png|jpg|jpeg|gif|webp|svg|ico|woff2|woff|ttf|otf|mp3|ogg|wav)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
